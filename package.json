{
  "name": "iobroker.oura",
  "version": "0.0.2",
  "description": "Oura Ring integration",
  "author": {
    "name": "Acgua",
    "email": "no@mail.com"
  },
  "homepage": "https://github.com/Acgua/ioBroker.oura",
  "license": "Apache-2.0",
  "keywords": [
    "oura",
    "ring",
    "health",
    "sleep"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Acgua/ioBroker.oura"
  },
  "dependencies": {
    "@iobroker/adapter-core": "^2.6.6",
    "axios": "^0.27.2"
  },
  "devDependencies": {
    "@alcalzone/release-script": "^3.5.9",
    "@iobroker/adapter-dev": "^1.1.0",
    "@iobroker/testing": "^4.1.0",
    "@types/chai": "^4.3.3",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.7.18",
    "@types/proxyquire": "^1.3.28",
    "@types/sinon": "^10.0.13",
    "@types/sinon-chai": "^3.2.8",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^10.0.0",
    "prettier": "^2.7.1",
    "proxyquire": "^2.1.3",
    "rimraf": "^4.4.1",
    "sinon": "^14.0.0",
    "sinon-chai": "^3.7.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typescript": "~4.8.3"
  },
  "main": "build/main.js",
  "files": [
    "admin{,/!(src)/**}/!(tsconfig|tsconfig.*).json",
    "admin{,/!(src)/**}/*.{html,css,png,svg,jpg,js}",
    "build/",
    "www/",
    "io-package.json",
    "LICENSE"
  ],
  "scripts": {
    "prebuild": "rimraf build",
    "build": "build-adapter ts",
    "watch": "build-adapter ts --watch",
    "prebuild:ts": "rimraf build",
    "build:ts": "build-adapter ts",
    "watch:ts": "build-adapter ts --watch",
    "test:ts": "mocha --config test/mocharc.custom.json src/**/*.test.ts",
    "test:package": "mocha test/package --exit",
    "test:unit": "mocha test/unit --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "npm run test:ts && npm run test:package",
    "check": "tsc --noEmit",
    "lint": "eslint --ext .ts src/",
    "translate": "translate-adapter",
    "release": "release-script"
  },
  "bugs": {
    "url": "https://github.com/Acgua/ioBroker.oura/issues"
  },
  "readmeFilename": "README.md"
}